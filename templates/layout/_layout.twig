{% do view.registerCssFile('/resources/css/index.min.css', {"async": "async", "defer": "defer", "media": "all"} )%}
{% do view.registerJsFile('/resources/js/index.min.js') %}

<!doctype html>
	<html lang="nb">
	<head>
		{% include "parts/_headStart" ignore missing %}
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width,initial-scale=1">
		<title>{{ entry['title'] is defined and entry.title and not entry.isHomepage ? entry.title : currentSite.name }}</title>
	</head>
	<body class="body{% if entry is defined %} body--{{ entry.type.handle }}{% endif %}">
		{% include "parts/_bodyStart" ignore missing %}
		<header class="header">
			{% block header %}
				{% include "parts/_header" ignore missing %}
			{% endblock %}
		</header>
		<main class="main{% if entry is defined and entry %} main--{{ entry.section.handle }}{% endif %}">
			{% block main %}{% endblock %}
		</main>
		<footer class="footer">
			{% block footer %}
				{% include "parts/_footer" ignore missing %}
			{% endblock %}
		</footer>
		{% include "parts/_bodyEnd" ignore missing %}
	</body>
</html>